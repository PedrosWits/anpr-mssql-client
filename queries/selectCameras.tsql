USE CortexDBWarehouse
SET NOCOUNT ON

SELECT  
    c.CameraID as id,
    c.Name as name,
    c.Latitude as latitude,
    c.longitude as longitude,
    c.Commissioned as isCommissioned,
    t.Description as type
FROM 
    cfg.Cameras as c
INNER JOIN
    static.CameraType as t ON c.CameraTypeID = t.CameraTypeID

GO
